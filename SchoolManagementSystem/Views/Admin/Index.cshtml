
@{
    ViewBag.Title = "AdminDashboard";
}

<div class="abc">
    <div class="wrapper">
        <aside class="main_sidebar">

            <ul>
                <li style="color: white"><h1 style="font-family: 'Raleway', sans-serif;">Admin</h1></li>
                <li><i class="glyphicon glyphicon-user"></i><a href="/Account/Register">Add Students</a></li>
                <li><i class="glyphicon glyphicon-user"></i><a href="/Account/RegisterT">Add Teachers</a></li>
                <li><i class="glyphicon glyphicon-book"></i><a href="/Admin/AddClasses">Add Classes</a></li>
                <li><i class="glyphicon glyphicon-book"></i><a href="/Admin/AddSections">Add Sections</a></li>
                <li><i class="glyphicon glyphicon-book"></i><a href="/Admin/AddSubjects">Add Subjects</a></li>
                <li><i class="glyphicon glyphicon-briefcase"></i><a href="/Admin/AssignClass">Assign Class</a></li>
                <li>
                    <div>
                        <a href="~/Admin/FeeChallans/" type="button" class="btn btn-primary btn-responsive">Get Pdf of Feechallans</a>
                    </div>
                </li>
               

            </ul>
        </aside>
        <div class="mian">
            <h1 style=" color: white;"> Prakash </h1>
        </div> 
    </div>

    <div class="abc">
        <div class="container">
            <div class="row">
                <div class="col-md-4">
                    <div class="dash-box dash-box-color-1">
                        <div class="dash-box-icon">
                            <i class="glyphicon glyphicon-cloud"></i>
                        </div>
                        <div class="dash-box-body">
                            <span class="dash-box-count">@ViewBag.student_count</span>
                            <span class="dash-box-title">Students</span>
                        </div>

                        <div class="dash-box-action">
                            <button onclick="window.location.href='/Admin/Studentlist'"> See List</button>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="dash-box dash-box-color-2">
                        <div class="dash-box-icon">
                            <i class="glyphicon glyphicon-download"></i>
                        </div>
                        <div class="dash-box-body">
                            <span class="dash-box-count">@ViewBag.teacher_count</span>
                            <span class="dash-box-title">Teachers</span>
                        </div>

                        <div class="dash-box-action">
                            <button onclick="window.location.href='/Admin/Teacherlist'">See List</button>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="dash-box dash-box-color-3">
                        <div class="dash-box-icon">
                            <i class="glyphicon glyphicon-heart"></i>
                        </div>
                        <div class="dash-box-body">
                            <span class="dash-box-count">@ViewBag.class_count</span>
                            <span class="dash-box-title">Classes</span>
                        </div>

                        <div class="dash-box-action">
                            <button onclick="window.location.href='/Admin/Classlist'">See List</button>
                        </div>
                    </div>
                </div>

                <div class="col-md-4">
                    <div class="dash-box dash-box-color-1">
                        <div class="dash-box-icon">
                            <i class="glyphicon glyphicon-download"></i>
                        </div>
                        <div class="dash-box-body">
                            <span class="dash-box-count">@ViewBag.section_count</span>
                            <span class="dash-box-title">Sections</span>
                        </div>

                        <div class="dash-box-action">
                            <button onclick="window.location.href='/Admin/Sectionlist'"> See List</button>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="dash-box dash-box-color-1">
                        <div class="dash-box-icon">
                            <i class="glyphicon glyphicon-cloud"></i>
                        </div>
                        <div class="dash-box-body">
                            <span class="dash-box-count">@ViewBag.subject_count</span>
                            <span class="dash-box-title">Subjects</span>
                        </div>

                        <div class="dash-box-action">
                            <button onclick="window.location.href='/Admin/Subjectlist'"> See List</button>
                        </div>
                    </div>
                </div>
                <div class="col-md-4">
                    <div class="dash-box dash-box-color-1">
                        <div class="dash-box-icon">
                            <i class="glyphicon glyphicon-cloud"></i>
                        </div>
                        <div class="dash-box-body">
                            <span class="dash-box-count">@ViewBag.subject_count</span>
                            <span class="dash-box-title">View Teacher's Attendance</span>
                        </div>

                        <div class="dash-box-action">
                            <button onclick="window.location.href='/Admin/TeacherAttendancelist'"> See List</button>
                        </div>
                    </div>
                </div>


            </div>
        </div>

        <ul class="pagination pg-blue">
            <li class="page-item">
                <a class="page-link" href="/Admin/Index" aria-label="Previous">
                    <span aria-hidden="true">&laquo;</span>
                    <span class="sr-only">Previous</span>
                </a>
            </li>
            <li class="page-item"><a class="page-link" href="/Admin/Index">1</a></li>
            <li class="page-item"><a class="page-link" href="/Admin/Index2">2</a></li>
            
            <li class="page-item">
                <a class="page-link" href="/Admin/Index2" aria-label="Next">
                    <span aria-hidden="true">&raquo;</span>
                    <span class="sr-only">Next</span>
                </a>
            </li>
        </ul>






      


    </div>

</div>
